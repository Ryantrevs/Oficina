@model List<Funcionario>
<table>
   <tr>
       <th>Nome</th>
       <th>Data de nascimento</th>
       <th>Função</th>
       <th>Contato</th>
       <th>Email</th>
   </tr>
    @{ 
        foreach (Funcionario func in Model){
        <tr>
            <td>@func.Nome</td>
            <td>@func.Nascimento</td>
            <td>@func.Funcao</td>
            <td>@func.PhoneNumber</td>
            <td>@func.Email</td>

        </tr>
        } 
    }
</table>